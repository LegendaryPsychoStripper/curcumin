
<title>Fa-mi cont!</title>

<!--<% if !current_page?(new_user_registration_path) %>-->

<!--<div style="display: none;">-->

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
 <%= devise_error_messages! %>
 
  <div class="field" >
    
    <%= f.email_field :email, placeholder: 'Email', style: "border-color: white;", id: 'printchatbox1' %>
  </div>

  <div class="field">
    
    
    <%= f.password_field :password,placeholder: 'Parola', style: "border-color: white;", autocomplete: "off", id: 'printchatbox3' %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> caractere minimum)</em>
    <% end %>
  </div>

<!--</div>-->
<center>
<div class="actions">
    <br/>
          <%= f.submit "Fa-mi cont!", class: 'submit-button round'%>
    
    <br/><br/>
  </div>
</center>
<%end%>


